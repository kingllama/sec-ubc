<div class="row">
    <div class="col-md-11 col-md-offset-1">
        <% for(var i=0; i< posts.length; i++) { %>
            <div>
                <a href="/blog/<%= posts[i].id %>" class="blog-header"><h1><%= posts[i].title %></h1></a>
                <div>
                    <%- posts[i].content %>
                </div>
            </div>
        <% } %>
    </div>
</div>